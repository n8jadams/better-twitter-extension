{
  "name": "Better Twitter Extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Removes the stuff I don't like from Twitter",
  "web_accessible_resources" : [
    "hideElements.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "https://*.twitter.com/*"
      ],
      "css" : [
        "hideElements.css"
      ],
      "js": [
        "execute.js"
      ],
      "run_at": "document_start",
      "all_frames": false
    }
  ]
}
